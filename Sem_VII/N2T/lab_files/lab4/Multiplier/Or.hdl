/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in= a, out= NotA);
    Not(in= b, out= NotB);
    And(a=NotA, b=NotB, out= NotAAndNotB);
    Not(in = NotAAndNotB, out= out);
}