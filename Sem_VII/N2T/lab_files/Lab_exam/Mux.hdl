/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=sel , out=notsel );
    And(a=notsel , b=a , out=anotsel );
    And(a=sel , b=b , out=bsel );
    Or(a= anotsel, b=bsel , out= out);
}